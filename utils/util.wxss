.course{
    padding: 12px 10px 0px 10px;
}
.course text.courseName{
    font-weight: bold;
    font-size: 22px;
}
.course text.modifyDate{
    float:right;
    color: lightgray;
    font-size: 15px;
    font-weight: lighter;
} 
.course text.num,.course text.createDate{
    color: darkgray;
    font-size: 16px;
    font-weight: lighter;
    margin-right:20px;
} 
.course text.teacherName{
    font-size: 16px;
    margin-left: 10px;
    color: black;
    font-weight: 200;
}

.customedTab label:nth-child(1){
    width:50%;
    display:inline-block;
    text-align:center;
}

.customedTab label:nth-child(2){
    width:50%;
    display:inline-block;
    text-align:center;
}

.customedTab{
    position:fixed;
    padding:5px;
    border-top:1rpx solid #D9D9D9;
    bottom:2px;
    width:100%;
    height:90rpx;
    background-color:white;
    font-size:13px
}

.blue{
    color:#286090;
}

.gray{
    color:rgb(181, 181, 181);
}